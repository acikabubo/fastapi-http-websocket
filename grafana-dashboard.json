{
  "dashboard": {
    "title": "FastAPI Application Metrics",
    "tags": ["fastapi", "prometheus"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} - {{status_code}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (95th percentile)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}} - p95"
          }
        ]
      },
      {
        "id": 3,
        "title": "Active WebSocket Connections",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "ws_connections_active",
            "legendFormat": "Active Connections"
          }
        ]
      },
      {
        "id": 4,
        "title": "WebSocket Connection Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(ws_connections_total[5m])",
            "legendFormat": "{{status}}"
          }
        ]
      },
      {
        "id": 5,
        "title": "WebSocket Message Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(ws_messages_received_total[5m])",
            "legendFormat": "Received"
          },
          {
            "expr": "rate(ws_messages_sent_total[5m])",
            "legendFormat": "Sent"
          }
        ]
      },
      {
        "id": 6,
        "title": "Rate Limit Hits",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(rate_limit_hits_total[5m])",
            "legendFormat": "{{limit_type}}"
          }
        ]
      },
      {
        "id": 7,
        "title": "HTTP Requests In Progress",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "http_requests_in_progress",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ]
      },
      {
        "id": 8,
        "title": "Application Info",
        "type": "table",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "app_info",
            "format": "table",
            "instant": true
          }
        ]
      }
    ],
    "refresh": "10s",
    "schemaVersion": 16,
    "version": 0
  }
}
